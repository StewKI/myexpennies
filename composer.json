{
    "autoload": {
        "psr-4": {
          "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "ext-pdo": "*",
        "doctrine/dbal": "^4.2",
        "doctrine/migrations": "^3.9",
        "doctrine/orm": "^3.5",
        "php-di/php-di": "^7.1",
        "slim/psr7": "^1.7",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.4",
        "symfony/cache": "^6.4",
        "symfony/console": "^6.4",
        "symfony/twig-bridge": "^6.4",
        "symfony/webpack-encore-bundle": "1.15.*",
        "twig/intl-extra": "^3.21",
        "twig/twig": "^3.21",
        "vlucas/phpdotenv": "^5.6",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
